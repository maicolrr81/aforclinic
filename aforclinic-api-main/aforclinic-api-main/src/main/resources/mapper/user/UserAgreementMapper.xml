<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xenialsoft.api.core.user.mapper.UserAgreementMapper">

    <insert id="insert">
        insert into user_agreements (
            user_id
          , type
          , agreed
        ) values (
            #{userId}
          , #{type}
          , #{agreed}
        )
    </insert>

</mapper>